snippet     reset
abbr        * {~}
options     head
  * {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
  }

  ul {
    list-style-type: none;
  }

  a {
    text-decoration: none;
    color: #000;
  }

snippet     flex
abbr        display: flex; ~
options     head
  display: flex;
  justify-content: ${1:space-between};

snippet     wh
abbr        width: ~; height: ~;
options     head
  width: ${1:0}px; height: ${2:0}px;
